mption from Statista\n\t\t\t\tvar conso = Math.round((6.39 + 7.31) \/2);\n\t\t\t\tvar km = Math.round(km_0);\n\t\t\t} else if (nb_vehicles == 1) {\n\t\t\t\tif (fuel_1 == 'Diesel') {\n\t\t\t\t\tif (conso_1 > 0) { var conso1 = conso_1; }\n\t\t\t\t\telse { var conso1 = 6.39; }\n            \t\tvar diesel_expenditures = km_1 * diesel_price * conso1 \/ 100;\n            \t\tvar gasoline_expenditures = 0;\n\t\t\t\t} else {\n\t\t\t\t\tif (conso_1 > 0) { var conso1 = conso_1; }\n\t\t\t\t\telse { var conso1 = 7.31; }\n            \t\tvar gasoline_expenditures = km_1 * gasoline_price * conso1 \/ 100;\n            \t\tvar diesel_expenditures = 0;\n\t\t\t\t}\n\t\t\t\tvar conso = Math.round(conso1);\n\t\t\t\tvar km = Math.round(km_1);\n\t\t\t} else if (nb_vehicles == 2) { \/\/ If the hh has several vehicles, we assume 2\/3 of the distance is made with the main one\n\t\t\t\tif (fuel_2_main == 'Diesel') {\n\t\t\t\t\tif (conso_2 > 0) { var conso2_1 = conso_2; }\n\t\t\t\t\telse { var conso_2_1 = 6.39; }\n            \t\tvar diesel_expenditures = (km_2 * 2 \/ 3) * diesel_price * conso2_1 \/ 100;\n            \t\tvar gasoline_expenditures = 0;\n\t\t\t\t} else {\n\t\t\t\t\tif (conso_2 > 0) { var conso2_1 = conso_2; }\n\t\t\t\t\telse { var conso2_1 = 7.31; }\n            \t\tvar gasoline_expenditures = (km_2 * 2 \/ 3) * gasoline_price * conso2_1 \/ 100;\n            \t\tvar diesel_expenditures = 0;\n\t\t\t\t}\n\t\t\t\tif (fuel_2_second == 'Diesel') {\n\t\t\t\t\tif (conso_2 > 0) { var conso2_2 = conso_2; }\n\t\t\t\t\telse { var conso2_2 = 6.39; }\n            \t\tdiesel_expenditures = diesel_expenditures + (km_2 * 1 \/ 3) * diesel_price * conso2_2 \/ 100;\n\t\t\t\t} else {\n\t\t\t\t\tif (conso_2 > 0) { var conso2_2 = conso_2; }\n\t\t\t\t\telse { var conso2_2 = 7.31; }\n            \t\tgasoline_expenditures = gasoline_expenditures + (km_2 * 1 \/ 3) * gasoline_price * conso2_2 \/ 100;\n\t\t\t\t}\n\t\t\t\tvar conso = Math.round(conso2_1 * 2 \/ 3 + conso2_2 * 1 \/ 3);\n\t\t\t\tvar km = Math.round(km_2);\n\t\t\t}\n\n\t\t\tvar diesel_increase = diesel_expenditures * ((1 - 0.4) * 0.090922);\n\t\t\tvar gasoline_increase = gasoline_expenditures * ((1 - 0.4) * 0.076128);\t\n\t\t\tvar heating_expenditures_increase = -55.507189 + gas * 124.578484 + fuel * 221.145441 + surface * 0.652174;\n\t\t\tvar energy_expenditures_increase = heating_expenditures_increase + diesel_increase + gasoline_increase;\n\t\t\n\t\t\tif (energy_expenditures_increase - 110 * Math.min(2, nb_adultes) > 16.1) {\n\t\t\t\tvar gain = 'perdant';\n\t\t\t\tvar winner = 0;\n\t\t\t} else {\n\t\t\t\tvar gain = 'gagnant';\n\t\t\t\tvar winner = 1;\n\t\t\t}\n\t\t\n\t\t\tif (gas) {\n\t\t\t\t$j('#type_chauffage').html(' au gaz ');\n\t\t\t} else if (fuel) {\n\t\t\t\t$j('#type_chauffage').html(' au fioul ');\n\t\t\t} else {\n\t\t\t\t$j('#type_chauffage').html(' ni au gaz, ni au fioul, ');\n\t\t\t}\n\t\t\t$j('#surface').html(' '+Math.round(surface));\n\t\t\t$j('#km').html(' '+km);\n\t\t\t\/\/$j('#surface').html(diesel_increase+gasoline_increase);\n\t\t\t$j('#conso').html(' '+conso);\n\t\t\t\/\/$j('#conso').html(' '+heating_expenditures_increase);\n\t\t\t$j('#gain').html(gain); \n\t\t\t\t\t\n\t\t\tQualtrics.SurveyEngine.setEmbeddedData('winner',winner);\n\t\t\tQualtrics.SurveyEngine.setEmbeddedData('nb_vehicles',nb_vehicles);\n\t\t\tQualtrics.SurveyEngine.setEmbeddedData('gas',gas);\n\t\t\tQualtrics.SurveyEngine.setEmbeddedData('fuel',fuel);\n\t\t\tQualtrics.SurveyEngine.setEmbeddedData('energy_expenditures_increase',energy_expenditures_increase);\n\t\t\tQualtrics.SurveyEngine.setEmbeddedData('diesel_increase',diesel_increase);\n\t\t\tQualtrics.SurveyEngine.setEmbeddedData('gasoline_increase',gasoline_increase);\n\t\t\tQualtrics.SurveyEngine.setEmbeddedData('heating_increase',heating_expenditures_increase);\n        });\n});"}},{"SurveyID":"SV_6s67PS7CwuYTuxD","Element":"QO","PrimaryAttribute":"QO_U4axGuddcATmC6u","SecondaryAttribute":"Occ","TertiaryAttribute":null,"Payload":{"Name":"Occ","Occurrences":101